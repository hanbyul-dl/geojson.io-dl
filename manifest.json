{
    "name": "DL + Geojson.io",
    "version": "1.0",
    "description": "DL + Geojson trial",
    "permissions": ["activeTab", "declarativeContent", "tabs","<all_urls>", "storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      }
    },
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "content_scripts": [
        {
        "matches": [
            "http://*/*",
            "https://*/*"
            ],
        "js": ["content.js"],
        "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
      "inject.js"
    ],
    "manifest_version":2
}
